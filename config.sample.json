{
    "box_image": "bento/ubuntu-24.04",
    "shared_dir": "_shared",
    "network": {
        "bridge_adapter": "en0: Wi-Fi",
        "subnet": "10.1.1",
        "ip_offset": 100,
        "cidr": 24,
        "gateway": "10.1.1.1"
    },
    "k8s": {
        "k8s_version": "1.33.3",
        "containerd_version": "2.1.3"
    },
    "node_counts": {
        "control_plane": 1,
        "worker": 1
    },
    "node_resources": {
        "control_plane": {
        "cpu": 2,
        "memory_mb": 4096
        },
        "worker": {
        "cpu": 2,
        "memory_mb": 4096
        }
    }
}